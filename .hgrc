[ui]
%include ~/.rc/.hgrc.local
ignore = ~/.rc/.hgignore.global
editor = hgeditor

[extensions]
hgext.convert=
rebase=
mq=
color=
progress=
record=
transplant=
graphlog=
pager=

[alias]
untrack = rm -Af
show = diff -ctip

[color]
mode = ansi

[pager]
pager = LESS='FRX' less
ignore = version, help, update, cat
